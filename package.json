{
  "name": "ts-boilerplate",
  "version": "1.0.0",
  "description": "TypeScript boilerplate",
  "main": "build/index.js",
  "files": [
    "build",
    "!build/**/*.test.*"
  ],
  "scripts": {
    "build": "npm run lint && npm run test && tsc",
    "dependencies:purge": "rm -rf node_modules && rm package-lock.json && npm install",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "npm run build",
    "prestart": "npm run build",
    "start": "node build/index.js",
    "test": "vitest run src",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "vitest watch src",
    "test:watch:ui": "npm run test:watch -- --ui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ulisesantana/ts-boilerplate.git"
  },
  "keywords": [],
  "author": "Ulises Santana <ulisesantana@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ulisesantana/ts-boilerplate/issues"
  },
  "homepage": "https://github.com/ulisesantana/ts-boilerplate#readme",
  "devDependencies": {
    "@types/node": "20.4.4",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "6.1.0",
    "@vitest/coverage-v8": "0.33.0",
    "@vitest/ui": "0.33.0",
    "eslint": "8.45.0",
    "eslint-config-standard-with-typescript": "37.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "16.0.1",
    "eslint-plugin-promise": "6.1.1",
    "typescript": "5.1.6",
    "vitest": "0.33.0"
  }
}
